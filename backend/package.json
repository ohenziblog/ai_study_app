{
  "name": "work_tsc_ai_project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "pnpm run start:dev",
    "start": "node dist/server.js",
    "build": "tsc",
    "start:dev": "nodemon --exec ts-node src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "pnpm typeorm migration:create src/migrations/InitialSchema",
    "migration:generate": "pnpm typeorm migration:generate -d src/config/DataSource.ts src/migrations/CreateTables",
    "migration:run": "pnpm typeorm migration:run -d src/config/DataSource.ts",
    "migration:revert": "pnpm typeorm migration:revert -d src/config/DataSource.ts",
    "migration:show": "pnpm typeorm migration:show -d src/config/DataSource.ts",
    "create:seed": "pnpm typeorm migration:create src/migrations/SeedEducationData",
    "db:seed": "cross-env NODE_ENV=development pnpm run migration:run",
    "db:reset": "pnpm run migration:revert && pnpm run migration:run",
    "test:user": "ts-node src/tests/userCrud.test.ts",
    "test": "ts-node src/tests/userCrud.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.1",
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "bcryptjs": "^3.0.2",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.87.4",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  }
}
